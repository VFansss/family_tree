<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>


  <context id="context1">
    <jdbcConnection connectionURL="jdbc:mysql://localhost:3306/collaborative_genealogy" 
      driverClass="com.mysql.jdbc.Driver"
      userId="root" />
      
    <javaModelGenerator targetPackage="it.univaq.ingweb.collaborative.db.dao.beans.user" targetProject="${basedir}\src\main\java" />
    <sqlMapGenerator targetPackage="it.univaq.ingweb.collaborative.db.dao.mappers.user" targetProject="${basedir}\src\main\java" />
    <javaClientGenerator targetPackage="it.univaq.ingweb.collaborative.db.dao.mappers.user" targetProject="${basedir}\src\main\java" type="XMLMAPPER" />
    <table tableName="user" domainObjectName="User">
      <columnOverride column="birthdate" jdbcType="TIMESTAMP" javaType="java.util.Date" />
    </table>
  </context>
</generatorConfiguration>

